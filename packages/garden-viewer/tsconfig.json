{
  "compilerOptions": {
    "jsx": "react-jsx",
    "lib": ["DOM", "ES2022"],
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "allowJs": false,
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true,
    "types": ["react", "react-dom"]
  },
  "include": ["src/**/*"]
}
